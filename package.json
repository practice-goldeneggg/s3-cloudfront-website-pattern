{
  "name": "vite2vue3ts4",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "serve": "vite preview",
    "repl": "ts-node",
    "build": "vue-tsc --noEmit && vite build && cp 403.html 404.html 50x.html dist/",
    "clean": "rimraf dist/*",
    "s3:up": "aws s3 sync dist/ s3://BUCKET-NAME/",
    "deploy": "npm-run-all build s3:up"
  },
  "dependencies": {
    "vue": "^3.0.5"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^1.2.2",
    "@vue/compiler-sfc": "^3.0.5",
    "typescript": "^4.1.3",
    "vite": "^2.3.3",
    "vue-tsc": "^0.0.24",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ts-node": "^10.1.0"
  }
}
